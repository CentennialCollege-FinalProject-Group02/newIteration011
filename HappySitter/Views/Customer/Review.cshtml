
@{
    ViewBag.Title = "Review";
}

<h1 class="panel-title" style="font-size:50px">Review</h1>
<br />
<br />
<div class="panel-body">
    <label class="label" style="color:black;">
        Sitter Name
        <input type="text" class="text-danger" />
    </label>
    <br />
    <br />
    <label class="label" style="color:black;">
        Comment
        <input type="text" class="text-danger" />
    </label>
    <br />
    <br />
    <label>Rate</label>
    <div class="form-group">
        <div class="radio">
            <label>
                <input type="radio" name="Rate" value="1">
                Level 1
            </label>
            <label>
                <input type="radio" name="identity" value="2" />
                Level 2
            </label>
            <label>
                <input type="radio" name="identity" value="3" />
                Level 3
            </label>
            <label>
                <input type="radio" name="identity" value="4" />
                Level 4
            </label>
            <label>
                <input type="radio" name="identity" value="5" checked="checked" />
                Level 5
            </label>


        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Confirm" class="btn btn-default" />
                <input type="button" value="Canel" class="btn btn-default" />

            </div>
        </div>

    </div>

